{
  "name": "rafalmasiarek/contact-form",
  "description": "Lightweight, pluggable contact form service with hooks, validators and PSR-7-friendly helpers.",
  "type": "library",
  "license": "MIT",
  "require": {
    "php": "^8.0",
    "psr/http-message": "^1.1 || ^2.0"
  },
  "suggest": {
    "phpmailer/phpmailer": "^6.9",
    "monolog/monolog": "^3.0"
  },
  "autoload": {
    "psr-4": {
      "rafalmasiarek\\ContactForm\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "rafalmasiarek\\ContactForm\\Tests\\": "tests/"
    }
  }
}